<script src="https://cdn.nierot.com/-/socket.io.js"></script>
<link id="style" rel="stylesheet" type="text/css" href="https://cdn.nierot.com/-/sakura-vader.css">
<meta http-equiv="Cache-control" content="no-cache">
<meta http-equiv="Expires" content="-1">
<script>
    localStorage.setItem('style', 'dark');
    const styles = [
        { url: 'https://cdn.nierot.com/-/sakura-dark.css', type: 'dark' },
        { url: 'https://cdn.nierot.com/-/sakura.css', type: 'light' },
        { url: 'https://cdn.nierot.com/-/sakura-earthly.css', type: 'light' },
        { url: 'https://cdn.nierot.com/-/sakura-vader.css', type: 'dark' },
        { url: 'https://cdn.nierot.com/-/sakura-dark-solarized.css', type: 'dark' },
        { url: 'https://cdn.nierot.com/-/sakura-ink.css', type: 'light' }
    ]

    function switchStyle() {
        let style = document.getElementById('style');
        let current = styles.findIndex(stylesheet => { return stylesheet.url === style.href });
        if (current === styles.length - 1) {
            style.href = styles[0].url;
            localStorage.setItem('style', styles[0].type);
        } else {
            style.href = styles[current + 1].url;
            localStorage.setItem('style', styles[current + 1].type);
        }
    }

</script>
<%- include('socket') %>
<meta charset="UTF-8">
<meta name="author" content="Nierot">
<meta name="viewport" content="width=device-width, initial-scale=1.0">