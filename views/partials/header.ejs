<script>
    const styles = [
        'https://cdn.nierot.com/-/sakura-dark.css',
        'https://cdn.nierot.com/-/sakura.css',
        'https://cdn.nierot.com/-/sakura-earthly.css',
        'https://cdn.nierot.com/-/sakura-vader.css',
        'https://cdn.nierot.com/-/sakura-dark-solarized.css',
        'https://cdn.nierot.com/-/sakura-ink.css'
    ]

    function switchStyle() {
        let style = document.getElementById('style');
        let current = styles.findIndex(stylesheet => { return stylesheet === style.href });
        if (current === styles.length - 1) {
            style.href = styles[0];
        } else {
            style.href = styles[current + 1];
        }

    }

</script>
